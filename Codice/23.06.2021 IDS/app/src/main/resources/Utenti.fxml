<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/14"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="it.unicam.cs.ids2021.view.JavaFXControllerUtenti">
   <children>
      <ButtonBar layoutX="89.0" layoutY="86.0" prefHeight="40.0" prefWidth="200.0" />
      <ButtonBar layoutX="113.0" layoutY="46.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
            <Button fx:id="buttonCassiere" mnemonicParsing="false" onAction="#CassiereScene" text="Cassiere" />
            <Button fx:id="buttonCorriere" mnemonicParsing="false" onAction="#CorriereScene" text="Corriere" />
          <Button fx:id="buttonAssiestenza" mnemonicParsing="false" onAction="#AssistenzaScene" text="Assistenza" />
            <Button fx:id="buttonCliente" mnemonicParsing="false" onAction="#ClienteScene" text="Cliente" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
